{
  "presets": [
    [
      "@babel/preset-env",
      {
        "debug": true,
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "not ie <= 10"]
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}
